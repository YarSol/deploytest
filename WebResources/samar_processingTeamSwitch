function processingTeamSwitch(executionContext) {
    var formContext = executionContext.getFormContext();
    var safeBrandValue = formContext.getAttribute("safe_brand").getValue();
    var brandName = "";
    if (safeBrandValue !== null) {
        brandName = safeBrandValue[0].name;
    }

    var processingTeam = formContext.getControl("samar_processingteam");
    var processingTeamValues = processingTeam.getAttribute().getOptions();
    processingTeam.clearOptions();

    processingTeamValues.forEach(function (element) {
        console.log("Processing Team Option:", element);
        if (brandName === "ISOQAR") {
            if (element.value === 779150001 || element.value === 779150002) {
                processingTeam.addOption(element);
            }
        }
    });
}
